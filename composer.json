{
    "name": "enricospa/microtime-tracker",
    "description": "A PHP micro library for time tracking",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "enricospa",
            "email": "spa.enrico@gmail.com"
        }
